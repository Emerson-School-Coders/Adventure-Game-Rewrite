{
	"game_loop": [
		[{
			"text": "You are walking in a forest and you find a chest!",
			"choice": {
				"options": [
					"Y",
					"N"
				],
				"text": "Do you open it?"
			},
			"type": "choice",
			"case": {
				"Y": {
					"type": "if",
					"condition": ["containsitems", "chests"],
					"if": {
						"true": {
							"text": "You open the chest!",
							"item_gain_random_wr": "chests",
							"text2": "You found a |RECENT|",
							"world_r_remove": true,
							"items_gone": "The chest was empty.",
							"type": "add_items"
						},
						"false": {
							"text": "The chest was empty.  You continue walking.",
							"type": "normal"
						}
					}
				},
				"N": {
					"text": "You choose to not open the chest.  You continue walking.",
					"type": "normal"
				}
			}
		}],
		[{
			"type": "normal",
			"text": "You continue walking."
		}],
		[{
			"type": "normal",
			"text": "The dread sets in... you're never going to get out of here, are you?"
		}],
		[{
			"type": "add_items",
			"item_gain_random_wr": "apple_tree",
			"text": "You find an apple tree.  You pick an apple."
		}],
        [{
            "type": "normal",
            "text": "A random old guy walks up to you in the forest while you are walking.  He yells \"YEET!\""
        }],
        [{
            "type": "choice",
            "text": "You come to a river.  Do you try to make a raft or do you lie a log across it?",
            "choice": {
                "options": [
                    "R",
                    "L"
                ],
                "text": "Raft or Log?"
            },
            "case": {
                "R": {
                    "type": "normal",
                    "text": "You find a raft and cross the river successfully."
                },
                "L": {
                    "type": "normal",
                    "text": "You cross the river, but after falling off of your log and getting scratched up.  You lose one life, you now have |HEALTH|.",
                    "health": -1
                }
            }
        }]
	],
